{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "max-line-length": [
            150
        ],
        "member-ordering": [
            false
        ],
        "object-literal-sort-keys": false,
        "no-console": [
            false
        ],
        "no-unused-variable": [
            true
        ],
        "curly": false
    },
    "rulesDirectory": []
}